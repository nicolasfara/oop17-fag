<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ProgressBar?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.layout.VBox?>

<AnchorPane style="-fx-background-color: rgba(41,41,41,0.2)" prefHeight="99.0" prefWidth="152.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1"
            stylesheets="/assets/ui/css/fag-style.css">
   <VBox layoutX="12.0" layoutY="10.0" prefHeight="79.0" prefWidth="129.0">
      <children>
          <ProgressBar fx:id="playerLife" prefHeight="20.0" prefWidth="137.0" progress="1.0" styleClass="playerLifeBar" />
         <GridPane fx:id="hud">
           <columnConstraints>
             <ColumnConstraints hgrow="SOMETIMES" maxWidth="136.0" minWidth="10.0" prefWidth="120.0" />
             <ColumnConstraints hgrow="SOMETIMES" maxWidth="150.0" minWidth="10.0" prefWidth="80.0" />
           </columnConstraints>
           <rowConstraints>
             <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
             <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
           </rowConstraints>
            <children>
                <Label fx:id="round" text="1" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                <Label fx:id="lblPointsTxt" text="Points:" />
                <Label fx:id="lblPoints" text="0" GridPane.columnIndex="1" />
                <Label fx:id="lblRoundTxt" text="Round:" GridPane.rowIndex="1" />
            </children>
         </GridPane>
      </children>
   </VBox>
</AnchorPane>
